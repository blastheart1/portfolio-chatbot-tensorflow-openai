{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install",
  "framework": "create-react-app",
  "public": true,
  "functions": {
    "api/send-lead.js": {
      "runtime": "nodejs18.x"
    }
  },
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=31536000,immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api|LuisBot\\.png|LuisBot\\.ico|code-light\\.ico|code-dark\\.ico|manifest\\.json|.*\\.(?:png|jpg|jpeg|gif|svg|ico|json|webp)).*)",
      "destination": "/index.html"
    }
  ]
}
